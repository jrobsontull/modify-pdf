{
  "name": "modify-pdf",
  "version": "0.5.0",
  "description": "Typesafe library for making simple modifications to PDFs.",
  "keywords": [
    "pdf",
    "typescript",
    ""
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jrobsontull/pdf-modify.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "jrobsontull",
  "bugs": {
    "url": "https://github.com/jrobsontull/pdf-modify/issues"
  },
  "homepage": "https://github.com/jrobsontull/pdf-modify#readme",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && rollup -c rollup.config.mjs",
    "start:dev": "rollup -c rollup.config.mjs -w",
    "lint": "eslint \"src/*.ts\"",
    "format": "prettier --write --config .prettierrc \"src/**/*.ts\" \"examples/**/*.ts\" \"examples/**/*.tsx\"",
    "test": "jest --coverage"
  },
  "dependencies": {
    "pdf-lib": "^1.17.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.43.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rollup": "^3.26.0",
    "rollup-plugin-typescript2": "^0.35.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/test/.*.(test|spec)).(ts|tsx|js)",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
